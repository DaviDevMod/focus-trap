{
  "name": "single-focus-trap",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/DaviDevMod/focus-trap/tree/main/packages/single-focus-trap",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf --no-glob ./dist && microbundle -f cjs,es --no-compress",
    "dev": "microbundle watch",
    "instrument": "rimraf --no-glob ./instrumented && nyc instrument --compact true ./src ./instrumented",
    "cyp": "rimraf --no-glob ./coverage ./.nyc_output && cypress open",
    "report-codecov": "nyc report --compact",
    "test": "yarn instrument && cypress run && yarn report-codecov"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/node": "^18.7.18",
    "cypress": "10.7.0",
    "cypress-real-events": "1.7.1",
    "eslint": "8.23.0",
    "istanbul-lib-coverage": "3.2.0",
    "microbundle": "0.15.1",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  },
  "keywords": [
    "focus",
    "trap",
    "TypeScript",
    "JavaScript"
  ],
  "author": {
    "name": "Davide Modica",
    "email": "davidevmod@gmail.com",
    "url": "https://github.com/DaviDevMod"
  }
}
