{
  "name": "single-focus-trap",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/DaviDevMod/focus-trap/tree/main/packages/single-focus-trap",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf --no-glob ./dist && microbundle -f cjs,es --no-compress",
    "dev": "microbundle watch",
    "instrument": "rimraf --no-glob ./instrumented && nyc instrument --compact true ./src ./instrumented",
    "src-to-instrumented": "replace-in-file \"/single-focus-trap/src/\" \"/single-focus-trap/instrumented/\" ../../apps/demo/tsconfig.json",
    "instrumented-to-src": "replace-in-file \"/single-focus-trap/instrumented/\" \"/single-focus-trap/src/\" ../../apps/demo/tsconfig.json",
    "clean-codecov": "rimraf --no-glob ./coverage ./.nyc_output",
    "cy-open": "yarn clean-codecov && cypress open",
    "cy-run": "yarn clean-codecov && cypress run",
    "report-codecov": "nyc report --compact",
    "e2e-open": "yarn instrument && yarn src-to-instrumented && concurrently -k \"yarn workspace demo dev-test\" \"yarn cy-open\"; yarn instrumented-to-src && yarn report-codecov",
    "e2e-run": "yarn instrument && yarn src-to-instrumented && concurrently -k \"yarn workspace demo dev-test\" \"yarn cy-run\"; yarn instrumented-to-src && yarn report-codecov"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.10.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/node": "18.7.21",
    "concurrently": "7.4.0",
    "cypress": "10.8.0",
    "cypress-real-events": "1.7.1",
    "eslint": "8.24.0",
    "istanbul-lib-coverage": "3.2.0",
    "microbundle": "0.15.1",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "replace-in-file": "6.3.5",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "keywords": [
    "focus",
    "trap",
    "TypeScript",
    "JavaScript"
  ],
  "author": {
    "name": "Davide Modica",
    "email": "davidevmod@gmail.com",
    "url": "https://github.com/DaviDevMod"
  }
}
