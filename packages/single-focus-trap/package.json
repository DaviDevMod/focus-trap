    "dev": "microbundle watch"
  },
  "devDependencies": {
    "eslint": "8.23.0",
    "microbundle": "0.15.0",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "4.7.4"
  },
  "keywords": [
    "focus",
    "trap",
    "TypeScript",
    "JavaScript"
