{
  "name": "use-simple-focus-trap",
  "version": "0.0.4",
  "license": "MIT",
  "repository": "https://github.com/DaviDevMod/focus-trap/tree/main/packages/use-simple-focus-trap",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf --no-glob ./dist && microbundle --alias @single-focus-trap=single-focus-trap --define process.env.NODE_ENV=production --tsconfig ./tsconfig.build.json -f cjs,es --no-compress",
    "dev": "microbundle watch"
  },
  "dependencies": {
    "single-focus-trap": "workspace:^"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "18.0.9",
    "eslint": "8.16.0",
    "eslint-config-react-app": "7.0.1",
    "microbundle": "0.15.0",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "4.7.4"
  },
  "keywords": [
    "use",
    "simple",
    "focus",
    "trap",
    "custom",
    "hook",
    "react",
    "TypeScript",
    "JavaScript"
  ],
  "author": {
    "name": "Davide Modica",
    "email": "davidevmod@gmail.com",
    "url": "https://github.com/DaviDevMod"
  }
}
