  "prettier": {
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "use-simple-focus-trap",
  "author": {
    "name": "Davide Modica",
    "email": "davidevmod@gmail.com",
    "url": "https://github.com/DaviDevMod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaviDevMod/use-simple-focus-trap"
  },
  "module": "dist/use-simple-focus-trap.esm.js",
  "size-limit": [
    {
      "path": "dist/use-simple-focus-trap.cjs.production.min.js",
